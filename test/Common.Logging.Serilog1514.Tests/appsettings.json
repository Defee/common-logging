{
    "common": {
        "logging": {
            "factoryAdapter": {
                "type": "SerilogLoggerFactoryAdapter, Common.Logging.Serilog"
            }
        }
    },
    "Serilog": {
        "Using": [ "Common.Logging.Serilog1514" ],
        "MinimumLevel": "Verbose",
        "WriteTo": [ "Console", "CommonLogging" ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    }
}

