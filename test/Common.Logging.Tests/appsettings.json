{
    "appSettings": {
        "appConfigCheck": "FromAppConfig"
    },
    "common": {
        "logging": {
            "factoryAdapter": {
                "type": "Common.Logging.Simple.ConsoleOutLoggerFactoryAdapter, Common.Logging",
                "arguments": {
                    "level": "DEBUG",
                    "showLogName": true,
                    "showDateTime": true,
                    "dateTimeFormat": "yyyy/MM/dd HH:mm:ss:fff"
                } 
            }
        }
    }
}
